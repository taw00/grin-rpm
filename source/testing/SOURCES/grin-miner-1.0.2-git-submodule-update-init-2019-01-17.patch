-impl fmt::Debug for Proof {
-	fn fmt(&self, f: &mut fmt::Formatter<'_>) -> fmt::Result {
-		write!(f, "Cuckoo{}(", self.edge_bits)?;
-		for (i, val) in self.nonces[..].iter().enumerate() {
-			write!(f, "{:x}", val)?;
-			if i < self.nonces.len() - 1 {
-				write!(f, " ")?;
-			}
-		}
-		write!(f, ")")
-	}
-}
