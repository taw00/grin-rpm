-			.queue(q.clone())
-			.len(el_count)
-			.fill_val(0xFFFFFFFF)
-			.build()?;
-		let counters = Buffer::<u32>::builder()
-			.queue(q.clone())
-			.len(el_count)
-			.fill_val(0)
-			.build()?;
